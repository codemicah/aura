# Smart Contract Environment Variables Template
# Copy this file to .env and fill in your actual values

# Deployment Private Key (DO NOT commit the actual .env file)
# This should be the private key of the account that will deploy and own the contracts
PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Snowtrace (Avalanche Block Explorer) API Key
# Required for contract verification
# Get your API key from: https://snowtrace.io/apis
SNOWTRACE_API_KEY=YOUR_SNOWTRACE_API_KEY_HERE

# RPC URLs (Optional - using default public RPCs if not specified)
# For better reliability, consider using dedicated RPC providers

# Avalanche Mainnet RPC URL
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc

# Avalanche Fuji Testnet RPC URL  
FUJI_RPC_URL=https://api.avax-test.network/ext/bc/C/rpc

# Local Development RPC URL (for anvil/hardhat)
LOCAL_RPC_URL=http://localhost:8545

# Protocol Integration Settings (Optional - using defaults if not specified)
# These can be used to override default protocol addresses for testing

# TraderJoe Router Address (mainnet)
TRADERJOE_ROUTER_ADDRESS=0x60aE616a2155Ee3d9A68541Ba4544862310933d4

# Benqi Comptroller Address (mainnet)
BENQI_COMPTROLLER_ADDRESS=0x486Af39519B4Dc9a7fCcd318217352830E8AD9b4

# YieldYak Farm Address (mainnet)  
YIELDYAK_FARM_ADDRESS=0xC4729E56b831d74bBc18797e0e17A295fA77488c

# Contract Deployment Settings
# Default yield rates in basis points (can be updated after deployment)
INITIAL_BENQI_APY=500     # 5%
INITIAL_TRADERJOE_APY=800 # 8%
INITIAL_YIELDYAK_APY=1200 # 12%

# Gas Settings (Optional)
# Uncomment and adjust if needed for deployment
# GAS_LIMIT=800000
# GAS_PRICE=25000000000  # 25 gwei

# Development/Testing Settings
# Set to true to enable additional logging during deployment
DEBUG_MODE=false

# Deployed Contract Addresses (will be filled after deployment)
# Keep these updated after successful deployments
YIELD_OPTIMIZER_MAINNET=
YIELD_OPTIMIZER_FUJI=
YIELD_OPTIMIZER_LOCAL=